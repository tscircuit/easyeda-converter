{
  "name": "easyeda",
  "type": "module",
  "version": "0.0.206",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/easyeda-converter"
  },
  "scripts": {
    "test": "bun test",
    "cli": "bun cli/main.ts",
    "build": "bun build:lib && bun build:cli && bun build:web",
    "build:lib": "tsup-node lib/index.ts --platform node --format esm --dts --sourcemap",
    "build:cli": "tsup cli/main.ts --platform node --format cjs --dts --sourcemap",
    "build:web": "tsup lib/websafe/index.ts --platform browser --format esm --dts --sourcemap -d dist/browser",
    "aider": "aider",
    "format:check": "biome format .",
    "format": "biome format . --write",
    "add-test-for-part": "bun run scripts/add-test-for-part.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "bin": {
    "easyeda-converter": "./dist/main.cjs",
    "easyeda": "./dist/main.cjs"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tscircuit/circuit-json-util": "^0.0.53",
    "@tscircuit/mm": "^0.0.8",
    "@types/bun": "latest",
    "bun-match-svg": "^0.0.6",
    "circuit-to-svg": "^0.0.164",
    "transformation-matrix": "^2.16.1",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.5.2",
    "tscircuit": "*",
    "zod": "*"
  }
}
