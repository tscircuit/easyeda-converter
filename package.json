{
  "name": "easyeda",
  "type": "module",
  "version": "0.0.199",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/easyeda-converter"
  },
  "scripts": {
    "test": "bun test",
    "cli": "bun cli/main.ts",
    "build": "bun build:node && bun build:web",
    "build:node": "tsup lib/index.ts cli/main.ts --platform node --format esm --dts --sourcemap",
    "build:web": "tsup lib/websafe/index.ts --platform browser --format esm --dts --sourcemap -d dist/browser",
    "aider": "aider",
    "format:check": "biome format .",
    "format": "biome format . --write",
    "add-test-for-part": "bun run scripts/add-test-for-part.ts"
  },
  "main": "./dist/lib/index.js",
  "exports": {
    ".": "./dist/lib/index.js",
    "./browser": "./dist/browser/index.js"
  },
  "bin": {
    "easyeda-converter": "./dist/cli/main.js",
    "easyeda": "./dist/cli/main.js"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tscircuit/circuit-json-util": "^0.0.50",
    "@types/bun": "latest",
    "bun-match-svg": "^0.0.6",
    "circuit-to-svg": "^0.0.164",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.5.2",
    "tscircuit": "*"
  },
  "dependencies": {
    "@tscircuit/mm": "^0.0.8",
    "commander": "^12.1.0",
    "transformation-matrix": "^2.16.1",
    "zod": "^3.25.36"
  }
}
